{
  "name": "whatsapp-remote",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "mount": "mount | grep $PWD/build || sudo mount -t tmpfs tmpfs ./build",
    "mount-dist": "mount | grep $PWD/dist || sudo mount -t tmpfs tmpfs ./dist",
    "watch": "nodemon --watch src --ext ts --exec node --env-file=.dev.env --enable-source-maps --import @swc-node/register/esm-register",
    "test": "npm run watch src/tests/index.test.ts",
    "dev": "npm run watch src/index.ts",
    "dev:panel": "vite",
    "build:panel": "vite build",
    "build:remote": "vite build -c vite.config.remote.ts",
    "build:src": "swc src -d build --strip-leading-paths --copy-files",
    "lint:src": "tsc -p ./tsconfig.node.json --noEmit",
    "clean": "rimraf ./build/*",
    "build": "npm run clean && npm run build:src && npm run build:panel && npm run build:remote",
    "prod": "cd build && node index",
    "dist": "./sea/build.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "express": "^5.1.0",
    "puppeteer-core": "^24.10.2",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@swc-node/register": "^1.10.10",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.2",
    "@types/express": "^5.0.3",
    "@types/node": "^22.0.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/controls": "^1.1.2",
    "@vue-flow/core": "^1.45.0",
    "@vue/tsconfig": "^0.7.0",
    "adm-zip": "^0.5.16",
    "codemirror": "^6.0.2",
    "nodemon": "^3.1.10",
    "postject": "^1.0.0-alpha.6",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "tar": "^7.4.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vite-plugin-vuetify": "^2.1.1",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "vuetify": "^3.8.10"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "type": "module"
}
